<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MasterServlet</servlet-name>
  	<display-name>MasterServlet</display-name>
  	<description></description>
  	<servlet-class>master.MasterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OrderServlet</servlet-name>
  	<display-name>OrderServlet</display-name>
  	<description></description>
  	<servlet-class>order.OrderServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SitServlet</servlet-name>
  	<display-name>SitServlet</display-name>
  	<description></description>
  	<servlet-class>sit.SitServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GarageServlet</servlet-name>
  	<display-name>GarageServlet</display-name>
  	<description></description>
  	<servlet-class>garage.GarageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MasterServlet</servlet-name>
  	<url-pattern>/masters</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OrderServlet</servlet-name>
  	<url-pattern>/orders</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SitServlet</servlet-name>
  	<url-pattern>/sits</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GarageServlet</servlet-name>
  	<url-pattern>/garages</url-pattern>
  </servlet-mapping>
</web-app>
